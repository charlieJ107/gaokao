@page
@model Gaokao.Areas.Admin.Pages.ProviceControlLine.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ControlLine[0].Provice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ControlLine[0].Year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ControlLine[0].Level)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ControlLine[0].Score)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ControlLine[0].Rank)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ControlLine) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Provice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Score)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rank)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
